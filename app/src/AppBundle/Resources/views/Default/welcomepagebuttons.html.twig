{# empty Twig template #}
<div class="col-sm-3"></div>
<div class="col-sm-3">
    <div {% if desktopbuttonimageafterlogin == "desktopbuttonimageafterloginlarge" %} class="desktopbutton" {% else %} class="desktopbuttonsmall" {% endif %} >
        <a href="{{ path('app_organization_create') }}">
            <img class="{{ desktopbuttonimageafterlogin }}" alt="Add organization" src="{{ asset('img/add_organization.svg') }}"/>
            <br/>
            <br/>
            Add organization
        </a>
    </div>
</div>
<div class="col-sm-3">
    {% if hasService is defined %}
        <div class="desktopbuttonsmall">
            <a href="{{ path('app_service_create') }}">
                <img class="{{ desktopbuttonimageafterlogin }}" alt="Claim service" src="{{ asset('img/add service.svg') }}"/>
                <br/>
                <br/>
                Claim service
            </a>
        </div>
    {% endif %}
</div>
<div class="col-sm-3">
    <div {% if desktopbuttonimageafterlogin == "desktopbuttonimageafterloginlarge" %} class="desktopbutton" {% else %} class="desktopbuttonsmall" {% endif %}>
        <a href="{{ path('app_profile_index') }}">
            <img class="{{ desktopbuttonimageafterlogin }}" alt="Edit profile" src="{{ asset('img/edit_profile.svg') }}"/>
            <br/>
            <br/>
            Edit profile
        </a>
    </div>
</div>